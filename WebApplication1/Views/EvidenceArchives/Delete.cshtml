@model WebApplication1.Models.Activities.EvidenceArchive

@{
    Layout = null;
}

<div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
        <div class="modal-header">Exlcuir arquivo</div>
        <div class="modal-body"><h3>Deseja realmente excluir este arquivo ? </h3></div>
        <div class="modal-footer">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                <input type="submit" value="Confirmar" class="btn btn-default" />
            }
        </div>
    </div>
</div>